{
  "name": "electron-react-typescript-boilerplate",
  "productName": "ElectronReactTypescript",
  "version": "0.1.1",
  "description": "Electron application boilerplate based on React, Typescript, React Router, Webpack, React Hot Loader for rapid application development",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development process.env.TS_NODE_DISABLE_WARNINGS=true node --max-old-space-size=4096 -r ts-node/register -r tsconfig-paths/register ./node_modules/webpack/bin/webpack --config src/webpack.config.renderer.dev.dll.ts --progress --profile --colors --display-error-details",
    "build-main": "cross-env NODE_ENV=production process.env.TS_NODE_DISABLE_WARNINGS=true node --max-old-space-size=4096 -r ts-node/register -r tsconfig-paths/register ./node_modules/webpack/bin/webpack --config src/webpack.config.main.prod.ts --progress --profile --colors --display-error-details",
    "build-renderer": "cross-env NODE_ENV=production process.env.TS_NODE_DISABLE_WARNINGS=true node --max-old-space-size=4096 -r ts-node/register -r tsconfig-paths/register ./node_modules/webpack/bin/webpack --config src/webpack.config.renderer.prod.ts --progress --profile --colors --display-error-details",
    "dev": "cross-env START_HOT=1 process.env.TS_NODE_DISABLE_WARNINGS=true npm run start-renderer-dev",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir src",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "package-linux": "npm run build && build --linux",
    "package-win": "npm run build && build --win --x64",
    "postinstall": "node -r ts-node/register src/scripts/CheckNativeDep.ts && npm run build-dll && electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development process.env.TS_NODE_DISABLE_WARNINGS=true electron -r ts-node/register -r tsconfig-paths/register ./src/Main/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node -r ts-node/register -r tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config src/webpack.config.renderer.dev.ts",
    "installer": "npm run package && node -r ts-node/register -r tsconfig-paths/register src/Main/utils/build.ts",
    "debug_prod": "cross-env DEBUG_PROD=true npm start",
    "analyze-build": "cross-env OPEN_ANALYZER=true npm run build",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r ts-node/register ./src/scripts/RunTests.ts",
    "test-all": "npm run build && npm run test && npm run test-e2e",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r ts-node/register ./src/scripts/RunTests.ts e2e",
    "test-watch": "npm test -- --watch"
  },
  "browserslist": "electron 1.6",
  "main": "./src/main.prod.js",
  "build": {
    "productName": "ElectronReactTypescript",
    "appId": "org.develar.ElectronReactTypescript",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "dir"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release",
      "app": "src"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManuKle/electron-react-typescript-boilerplate.git"
  },
  "author": {
    "name": "Manuel Klettke",
    "email": "manuelklettke@web.de",
    "url": "https://github.com/ManuKle"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ManuKle/electron-react-typescript-boilerplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "typescript",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/ManuKle/electron-react-typescript-boilerplate#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/src/Main/utils/tempPolyFills.ts",
      "<rootDir>/src/Main/utils/enzyme_setup.ts",
      "<rootDir>/src/scripts/CheckBuildsExist.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "@act/(.*)": "<rootDir>/src/Renderer/actions/$1", 
      "@con/(.*)": "<rootDir>/src/Renderer/containers/$1", 
      "@com/(.*)": "<rootDir>/src/Renderer/components/$1", 
      "@red/(.*)": "<rootDir>/src/Renderer/reducers/$1", 
      "@utils/(.*)": "<rootDir>/src/Main/utils/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ]
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.0.1",
    "@types/detect-port": "^1.1.0",
    "@types/electron-debug": "^1.1.0",
    "@types/enzyme": "^3.1.4",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.1.1",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.13",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "@types/react-router-redux": "^5.0.10",
    "@types/redux-logger": "^3.0.5",
    "@types/webpack": "^3.8.1",
    "@types/webpack-bundle-analyzer": "^2.9.0",
    "@types/webpack-env": "^1.13.2",
    "@types/webpack-merge": "^4.1.1",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.9",
    "detect-port": "^1.2.2",
    "electron": "^1.7.11",
    "electron-builder": "^19.55.3",
    "electron-devtools-installer": "^2.2.3",
    "electron-rebuild": "^1.7.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fbjs-scripts": "^0.8.1",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.7.2",
    "npm-logical-tree": "^1.2.1",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.1",
    "stylefmt": "^6.0.0",
    "ts-jest": "^22.0.3",
